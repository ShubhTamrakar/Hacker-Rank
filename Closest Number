int closestNumber(int a, int b, int x) {
    long long p = (long long) round(pow(a, b));
    long long low = (p / x) * x;
    long long high = (p >= 0 ? low + x : low - x);
    long long diffLow = llabs(p - low);
    long long diffHigh = llabs(high - p);

    if (diffLow < diffHigh) return (int)low;
    else if (diffLow > diffHigh) return (int)high;
    else return (int)(low < high ? low : high);
}
//https://www.hackerrank.com/challenges/closest-number/problem
